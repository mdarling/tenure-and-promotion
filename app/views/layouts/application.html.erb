<!DOCTYPE html>
<html>
<head>
  <title>UNM Tenure and Promotion</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= nav_bar do %>
 <%= menu_group do %>
        <%= menu_item "Start", users_path %>
        <%= menu_divider %>
        <%= menu_item "Contact", contact_path %>
        <%= menu_item "Log out " + session[:cas_user], "https://login.unm.edu/cas/logout" %>
    <% end %>
<% end %>
<div class="container">
<%= yield %>
</div>
</body>
</html>
