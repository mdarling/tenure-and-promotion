<h1>Listing votes</h1>
  
      <h5>Vote</h5>
      <h5>Review</h5>
      <h5>Review candidate profile</h5>
      <h5>User</h5>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.vote_id %></td>
        <td><%= vote.Review_id %></td>
        <td><%= vote.Review_Candidate_Profile_id %></td>
        <td><%= vote.user_id %></td>
        <td><%= link_to 'Show', vote %></td>
        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %></br>
